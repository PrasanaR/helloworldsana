pipeline{
  agent any
    
    stages{
    
      stage('SonarCloud') {
  environment {
 SCANNER_HOME = tool 'SonarQubeScanner'
    
    PROJECT_NAME = "jenkins"
  }
  steps {
    withSonarQubeEnv('SonarQubeServer') {
        sh '''$SCANNER_HOME/bin/sonar-scanner \
        -Dsonar.java.binaries=build/classes/java/ \
        -Dsonar.projectKey=$PROJECT_NAME \
        -Dsonar.sources=.'''
    }
  }
}
      
      }
      
   }
