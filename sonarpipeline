pipeline{
  agent any
    
    stages{
    
      stage('SonarCloud') {
  environment {
    SCANNER_HOME = tool 'SonarQubeScanner'
    PROJECT_NAME = "igorstojanovski_jenkins-pipeline-as-code"
  }
  steps {
    withSonarQubeEnv('SonarQubeScanner') {
        sh '$SCANNER_HOME/bin/sonar-scanner'
    }
  }
}
      
      }
      
   }
